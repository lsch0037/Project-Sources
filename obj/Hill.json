{
    "HalfBaseWidth":"!div($BaseWidth, 2)",
    "Gradient":"!div($MaxHeight, !pow($HalfBaseWidth, 2))",
    "Loop":{
        "Var":"x_iter",
        "Start":"!sub(0, $HalfBaseWidth)",
        "End":"$HalfBaseWidth",
        "Body":{
            "Loop":{
                "Var":"z_iter",
                "Start":"!sub(0, $HalfBaseWidth)",
                "End":"$HalfBaseWidth",
                "Body":{
                    "Offset":[
                        ["$x_iter",0,"$z_iter"],
                        {
                            "Pos":["$x_iter", 0, "$z_iter"],
                            "BaseHeight":"!sub($MaxHeight, !mul($Gradient,!add(!pow($x_iter, 2), !pow($z_iter, 2))))",
                            "PerlinHeight":"!perlin($Pos, $Oct, $Seed)",
                            "Cuboid":{
                                "Dimensions":[1, "!add($BaseHeight, !mul($RandomNess, $PerlinHeight))", 1],
                                "Material":{
                                    "Selector":"Random",
                                    "Ids":["Grass Block"]
                                }
                            }
                        }
                    ]
                }
            }
        }
    }
}