{
    "Loop":{
        "loop_var":"x_iter",
        "loop_range":"$BaseWidth",
        "loop_body":{
            "Loop":{
                "loop_var":"z_iter",
                "loop_range":"$BaseWidth",
                "loop_body":{
                    "x_pos":"!sub(!div($BaseWidth, 2),$x_iter)",
                    "z_pos":"!sub(!div($BaseWidth, 2),$z_iter)",
                    "BaseHeight":"!sub(10,!mul(0.17,!add(!pow($x_pos, 2), !pow($z_pos, 2))))",
                    "Offset":[
                        ["$x_iter",0,"$z_iter"],
                        {
                            "Cuboid":{
                                "Dimensions":[1, "$BaseHeight",1],
                                "Material":{
                                    "Selector":"Random",
                                    "Ids":["Grass Block"]
                                }
                            }
                        }
                    ]
                }
            }
        }
    }
}