{
    "On": [
        {
            "TreeHeight": 20,
            "TrunkThickness": 3,
            "CrownSize": 5,
            "WoodType": {
                "Selector": "Random",
                "Ids": [
                    "Oak Wood"
                ]
            },
            "LeavesType": {
                "Selector": "Random",
                "Ids": [
                    "Oak Leaves"
                ]
            },
            "Tree": {
                "On Ground": {
                    "On": [
                        {
                            "Sphere": {
                                "Radius": "$CrownSize",
                                "Material": "$LeavesType"
                            }
                        },
                        {
                            "Cylinder": {
                                "Radius": "$TrunkThickness",
                                "Length": "$TreeHeight",
                                "Material": "$WoodType"
                            }
                        }
                    ]
                }
            }
        },
        {
            "HouseHeight": 10,
            "HouseLength": 10,
            "HouseWidth": 10,
            "WallThickness": 1,
            "WallMaterial": {
                "Selector": "Random",
                "Ids": [
                    "Bricks"
                ]
            },
            "FloorMaterial": {
                "Selector": "Random",
                "Ids": [
                    "Stone"
                ]
            },
            "RoofMaterial": {
                "Selector": "Random",
                "Ids": [
                    "Bricks"
                ]
            },
            "House": {
                "On": [
                    {
                        "RoofWidth": "$HouseWidth",
                        "RoofHeight": "!div($HouseWidth,2)",
                        "RoofLength": "$HouseLength",
                        "HalfRoofLength": "!div($RoofLength,2)",
                        "HalfRoofWidth": "!div($RoofLength,2)",
                        "2SideSquared": "!add(!pow($RoofHeight,2),!pow($HalfRoofWidth,2))",
                        "Hypotenuse": "!sqrt($2SideSquared)",
                        "Offset": [
                            [
                                "!sub(0,$HalfRoofLength)",
                                "$RoofHeight",
                                0
                            ],
                            {
                                "Union": [
                                    {
                                        "Rotation": {
                                            "Axis": "x",
                                            "Degrees": 45.0,
                                            "Body": {
                                                "Cuboid": {
                                                    "Dimensions": [
                                                        "$RoofLength",
                                                        1,
                                                        "$Hypotenuse"
                                                    ],
                                                    "Material": "$RoofMaterial"
                                                }
                                            }
                                        }
                                    },
                                    {
                                        "Rotation": {
                                            "Axis": "x",
                                            "Degrees": 135.0,
                                            "Body": {
                                                "Cuboid": {
                                                    "Dimensions": [
                                                        "$RoofLength",
                                                        1,
                                                        "$Hypotenuse"
                                                    ],
                                                    "Material": "$RoofMaterial"
                                                }
                                            }
                                        }
                                    }
                                ]
                            }
                        ]
                    },
                    {
                        "RoomHeight": "$HouseHeight",
                        "RoomLength": "$HouseLength",
                        "RoomWidth": "$HouseWidth",
                        "HalfRoomLength": "!div($RoomLength,2)",
                        "HalfRoomWidth": "!div($RoomWidth,2)",
                        "Offset": [
                            [
                                "!sub(0,$HalfRoomLength)",
                                0,
                                "!sub(0,$HalfRoomWidth)"
                            ],
                            {
                                "Difference": [
                                    {
                                        "Cuboid": {
                                            "Dimensions": [
                                                "$RoomLength",
                                                "$RoomHeight",
                                                "$RoomWidth"
                                            ],
                                            "Material": "$WallMaterial"
                                        }
                                    },
                                    {
                                        "InnerLength": "!sub($RoomLength,!mul($WallThickness,2))",
                                        "InnerWidth": "!sub($RoomWidth,!mul($WallThickness,2))",
                                        "Offset": [
                                            [
                                                "$WallThickness",
                                                1,
                                                "$WallThickness"
                                            ],
                                            {
                                                "Cuboid": {
                                                    "Dimensions": [
                                                        "$InnerLength",
                                                        "!sub($RoomHeight,2)",
                                                        "$InnerWidth"
                                                    ],
                                                    "Material": "$WallMaterial"
                                                }
                                            }
                                        ]
                                    }
                                ]
                            }
                        ]
                    }
                ]
            }
        }
    ]
}